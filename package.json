{
  "name": "insta-vids-downloader",
  "version": "1.0.0",
  "description": "Download instagram videos with a simple click",
  "main": "src/app.js",
  "scripts": {
    "start": "babel-node src/app.js",
    "dev": "nodemon --exec babel-node src/app.js",
    "build": "babel src --out-dir dist --copy-files",
    "prod": "pm2 start dist/app.js --name insta-express",
    "css": "npx tailwindcss -i ./public/css/tailwind.css -o ./public/css/style.css --watch"
  },
  "author": "Riadh Azzoun",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "pug": "^3.0.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "serve-static": "^1.15.0",
    "better-sqlite3": "^8.2.0",
    "better-sqlite3-session-store": "^0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "nodemon": "^2.0.22",
    "tailwindcss": "^3.3.1"
  }
}
